{
    "gitlens.gitExplorer.enabled": false,
    "search.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/vendor/composer/autoload_*.php": true,
        "**/build_*": true,
        "**/_tmp": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/vendor/composer/autoload_*.php": true,
        "**/build_*": true,
        "**/_tmp": true
    },
    "explorer.openEditors.visible": 0,
    "projectManager.treeview.visible": false,
    "cSpell.words": [
        "a",
        "blank",
        "body",
        "box",
        "c",
        "class",
        "com",
        "d",
        "dholloran",
        "feed",
        "footer",
        "github",
        "h",
        "href",
        "https",
        "icon",
        "in",
        "l",
        "li",
        "linked",
        "linkedin",
        "m",
        "path",
        "rss",
        "s",
        "span",
        "subscribe",
        "svg",
        "target",
        "twitter",
        "ul",
        "username",
        "v",
        "view",
        "xml",
        "z"
    ],
    "workbench.colorCustomizations": {
        "list.focusBackground": "#6a7d89",
        "list.focusForeground": "#fff",
        "titleBar.activeBackground": "#81c1c9",
        "titleBar.inactiveBackground": "#81c1c999",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveForeground": "#15202b99"
    }
}
